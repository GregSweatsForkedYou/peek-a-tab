{
  "manifest_version": 3,
  "name": "Peek-a-tab, Tabs Manager for Google Chromeâ„¢",
  "description": "Search, preview, jump across, and close tabs quickly in your chrome browser.",
  "version": "1.5.6",
  "icons": {
    "128": "icon-5.png"
  },
  "action": {
    "default_icon": "icon-5.png",
    "default_popup": "popup.html"
  },
  "permissions": ["tabs", "storage", "scripting"],
  "offline_enabled": true,
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "windows": "Alt+Space",
        "mac": "Alt+Space",
        "chromeos": "Alt+Space",
        "linux": "Alt+Space"
      }
    }
  }
}
